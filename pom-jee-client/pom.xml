<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/xsd/maven-4.0.0.xsd">

	<parent>
		<groupId>org.cyk.pom.jee</groupId>
		<artifactId>pom-jee</artifactId>
		<version>pom.jee.0.1.0</version>
	</parent>

	<name>JEE client project object model</name>
	<url>http://www.cykcorp.org</url>
	<modelVersion>4.0.0</modelVersion>
	<groupId>org.cyk.pom.jee.client</groupId>
	<artifactId>pom-jee-client</artifactId>
	<version>pom.jee.client.0.1.0</version>
	<packaging>pom</packaging>
	
	<profiles>
		<profile>
			<id>user.interface.theme</id>
		</profile>
		
		<profile>
			<id>user.interface.theme.web</id>
			<properties>
				<cyk.context.parameter.theme.favicon.file.folder>image</cyk.context.parameter.theme.favicon.file.folder>
				<cyk.context.parameter.theme.favicon.file.name.prefix>favicon</cyk.context.parameter.theme.favicon.file.name.prefix>
				<cyk.context.parameter.theme.favicon.file.name.extension>png</cyk.context.parameter.theme.favicon.file.name.extension>
				
				<cyk.context.parameter.theme.logo.file.folder>image</cyk.context.parameter.theme.logo.file.folder>
				<cyk.context.parameter.theme.logo.file.name>logo.png</cyk.context.parameter.theme.logo.file.name>
				<cyk.context.parameter.theme.logo.file.name.prefix>logo</cyk.context.parameter.theme.logo.file.name.prefix>
				<cyk.context.parameter.theme.logo.file.name.extension>png</cyk.context.parameter.theme.logo.file.name.extension>
			</properties>
		</profile>
		
		<profile>
			<id>user.interface.theme.web.jsf</id>
		</profile>
		
		<profile>
			<id>user.interface.theme.web.jsf.primefaces</id>
			<properties>
				<cyk.context.parameter.theme.class.name>org.cyk.utility.client.controller.web.jsf.primefaces.theme.ThemeDesktopDefault</cyk.context.parameter.theme.class.name>
				<cyk.context.parameter.theme.primefaces>omega</cyk.context.parameter.theme.primefaces>
			</properties>
		</profile>
		
		<profile>
			<id>user.interface.theme.web.jsf.primefaces.atlantis</id>
			<properties>
				<cyk.context.parameter.theme.class.name>org.cyk.user.interface_.theme.web.jsf.primefaces.atlantis.DesktopDefault</cyk.context.parameter.theme.class.name>
				<cyk.context.parameter.theme.primefaces>atlantis-blue</cyk.context.parameter.theme.primefaces>
			</properties>
			<dependencies>
				<dependency>
		            <groupId>org.cyk.jee.user.interface.theme.web.jsf.primefaces.atlantis</groupId>
					<artifactId>jee-user-interface-theme-web-jsf-primefaces-atlantis</artifactId>
    				<version>0.0.1</version>
		        </dependency>
			</dependencies>
		</profile>
		
		<profile>
			<id>user.interface.theme.web.jsf.primefaces.adminfaces</id>
			<properties>
				<cyk.context.parameter.theme.class.name>org.cyk.user.interface_.theme.web.jsf.primefaces.adminfaces.DesktopDefault</cyk.context.parameter.theme.class.name>
				<cyk.context.parameter.theme.primefaces>admin</cyk.context.parameter.theme.primefaces>
			</properties>
			<dependencies>
				<dependency>
		            <groupId>org.cyk.jee.user.interface.theme.web.jsf.primefaces.adminfaces</groupId>
					<artifactId>jee-user-interface-theme-web-jsf-primefaces-adminfaces</artifactId>
    				<version>0.0.1</version>
		        </dependency>
			</dependencies>
		</profile>
	</profiles>
	
</project>
